{
  "name": "@bbloker/sdk",
  "version": "0.4.0",
  "type": "module",
  "description": "AI bot blocker SDK â€” shared intelligence network to detect and block AI crawlers",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./nextjs": {
      "types": "./dist/adapters/nextjs.d.ts",
      "import": "./dist/adapters/nextjs.js",
      "require": "./dist/adapters/nextjs.cjs"
    },
    "./express": {
      "types": "./dist/adapters/express.d.ts",
      "import": "./dist/adapters/express.js",
      "require": "./dist/adapters/express.cjs"
    },
    "./hono": {
      "types": "./dist/adapters/hono.d.ts",
      "import": "./dist/adapters/hono.js",
      "require": "./dist/adapters/hono.cjs"
    },
    "./node": {
      "types": "./dist/adapters/node.d.ts",
      "import": "./dist/adapters/node.js",
      "require": "./dist/adapters/node.cjs"
    },
    "./fastify": {
      "types": "./dist/adapters/fastify.d.ts",
      "import": "./dist/adapters/fastify.js",
      "require": "./dist/adapters/fastify.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bot-blocker",
    "ai-crawler",
    "middleware",
    "security",
    "web-scraping",
    "gptbot",
    "claudebot"
  ],
  "author": "Caio Ricciuti",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "hono": "^4.11.8",
    "next": "^16.1.6",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "hono": ">=4.0.0",
    "next": ">=13.0.0",
    "fastify": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "hono": {
      "optional": true
    },
    "fastify": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
